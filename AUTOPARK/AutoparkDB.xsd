	ON [Личный состав].[табельный  номер] = [Путевой лист Грузового авто].ID_Водителя 
INNER JOIN Отделы 
	ON [Путевой лист Грузового авто].ID_Отдела = Отделы.Код 
INNER JOIN [Подвижной состав] 
	ON [Путевой лист Грузового авто].ID_Автомобиля = [Подвижной состав].ID 
LEFT JOIN
    (SELECT [ID_Путевого листа], 
