                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  [Путевой лист Грузового авто].[Дата путевого листа] AS 'Дата_путевого_листа', 
	[Путевой лист Грузового авто].[Номер путевого листа] AS 'Номер_путевого_листа', 
    [Личный состав].Фамилия + ' ' + LEFT([Личный состав].Имя, 1) + '. ' + LEFT([Личный состав].Отчество, 1) + '. ' AS 'ФИО', 
	Отделы.Подразделение, 
    [Путевой лист Грузового авто].Пункт_отправления,
	[Путевой лист Грузового авто].Пункт_назначения,
	ISNULL(zapr.Запрaвлено, 0) AS 'Заправлено_ТСМ1',
	ISNULL(zapr2.Запрaвлено2, 0) AS 'Заправлено_ТСМ2',
    [Путевой лист Грузового авто].Остаток_ТСМ1_выезд,
	[Путевой лист Грузового авто].Остаток_ТСМ1_возвр, 
	[Путевой лист Грузового авто].Остаток_ТСМ2_выезд, 
    [Путевой лист Грузового авто].Остаток_ТСМ2_возвр,
    CONVERT(time,[Путевой лист Грузового авто].Дата_Время_выезда_факт) AS 'Дата_Время_выезда_факт',
    CONVERT(time,[Путевой лист Грузового авто].Дата_Время_возвращения_факт) AS 'Дата_Время_возвращения_факт', 
	[Путевой лист Грузового авто].Показания_спидометра_при_выезде,
	[Путевой лист Грузового авто].Показания_спидометра_при_возвращении 
